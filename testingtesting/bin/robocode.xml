<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="RoboCode">
			<graphics key="collapsed" value="false"/>
			<alt name="Movement">
				<and name="Random_Movement">
					<feature name="Eighty_Twenty_Switch"/>
				</and>
				<alt name="Pattern_Movement">
					<feature name="Corner_Movement"/>
					<feature name="Stop_n_Go"/>
					<feature name="Spin"/>
				</alt>
				<and name="Wave_Surfing">
					<graphics key="collapsed" value="false"/>
					<or mandatory="true" name="Implementation">
						<feature name="Range_Search"/>
						<feature name="kNN_GF"/>
						<feature name="Dynamic_Clustering"/>
					</or>
					<feature name="GoTo"/>
					<feature name="Melee_surf"/>
				</and>
				<feature name="Minimum_Risk_Movement"/>
			</alt>
			<or name="Memory">
				<feature name="GF_Memory"/>
				<feature name="Velocities"/>
			</or>
			<alt name="Targeting">
				<and name="DynamicClustering">
					<feature name="Play_It_Forward"/>
				</and>
				<or name="PatternMatching">
					<feature name="Symbolic_PM"/>
					<feature name="Nano_style"/>
				</or>
				<and name="GuessFactor">
					<feature name="kNN_GF_Targeting"/>
				</and>
				<feature name="Head_On_Targeting"/>
				<feature name="Random"/>
			</alt>
			<and mandatory="true" name="Radar">
				<alt name="ChooseEnemy">
					<feature name="Nearest_Enemy"/>
					<feature name="Most_Likely_To_Hit"/>
				</alt>
			</and>
			<alt name="BattleMode">
				<feature name="One_v_One"/>
				<feature name="Melee"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>GF_Memory</var>
				<disj>
					<var>GuessFactor</var>
					<var>kNN_GF</var>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Velocities</var>
				<var>Movement</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>ChooseEnemy</var>
				<var>Melee</var>
			</imp>
		</rule>
	</constraints>
</featureModel>
